# up/down arrow history completion based on what you've typed
"\e[A": history-search-backward
"\e[B": history-search-forward

# control + <- / ->
"\e[1;5D": backward-word
"\e[1;5C": forward-word
# and option + <- / ->
"\e\e[D": backward-word
"\e\e[C": forward-word

# make the home and end keys do the right thing
"\e[H": beginning-of-line
"\e[F": end-of-line
